<div id=multi_column_1 class="options_column_40">
    <div class="btn_panel_general">
        <div id="large_panel_text" class="ops_left_header">
            <p>XSTR("IP Address", 888176)</p>
        </div>
		<ul id=ipaddress_list>
		</ul>
        <div id=ipaddress_controls_wrapper>
            <div id="ipaddress_controls">
				<div id=add_remove_cont>
                    <button id="remove_btn" class="controls button_1" onclick="controller:remove_selected_ip()">
                        <div id="remove_img" class="controls button_img">
                            <img src="exit.png" class="psuedo_img"></img>
                        </div>
                        <div class="neg button_text">
                            <p>XSTR("Del", 888177)</p>
                        </div>
                    </button>
                </div>
                <div id=add_remove_cont>
                    <button id="add_btn" class="controls button_1" onclick="controller:add_selected_ip()">
