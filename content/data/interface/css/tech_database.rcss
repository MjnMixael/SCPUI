#angle_slider #angle_range_cont
{
	width: 100%;
}

#angle_slider input.range
{
	width: 65%;
	height: 100%;
	scrollchange-sound: 9;
	position: absolute;
}

#angle_slider input.range slidertrack
{
	background-decorator: image;
	background-image: scroll-bar-line.png;
	height: 2%;
