}

#angle_slider input.range sliderbar:active
{
	background-image: scroll-button-o.png;
}

#angle_slider input.range sliderarrowdec
{
	width: 100%;
	height: 10%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-up.png;
}

#angle_slider input.range sliderarrowinc
{
	width: 100%;
	height: 10%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-down.png;
}

#angle_slider input.range sliderarrowdec:hover
{
