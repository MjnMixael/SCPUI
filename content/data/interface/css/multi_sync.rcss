body
{
	z-index: bottom;
	z-index: -1;
}

.below p
{
	padding-top:8%;
}

/*Players List*/

.player_item
{
	width: 40%;
	float: left;
}

#players_wrapper
{
	width: 20%;
	height: 60%;
	position: absolute;
	top: 10%;
	left: 15%;
}

div#players_wrapper img
{
	display: block;
	width: 100%;
	height: 100%;
}

#players_div
{
	width: 82%;
	height: 86%;
	position: absolute;
	top: 10%;
	left: 16%;
	overflow: scroll;
}

/*Chat Window*/
#chat_wrapper
{
	width: 60%;
	height: 60%;
	position: absolute;
	top: 10%;
	right: 0%;
}

div#chat_wrapper img
{
	display: block;
	width: 100%;
	height: 100%;
}

#chat_window
{
	width: 94%;
	height: 92%;
	position: absolute;
	top: 5%;
	left: 2%;
	overflow: scroll;
}

#chat_input
{
	position:absolute;
	bottom: -7%;
	left: 0%;
	height: 7%;
	width: 75%%;
	/*font-size: 1em;*/
	background-decorator: image;
	background-image: input-box.png;
	border-width: 1px;
	border-color: #676767;
	line-height: 0.9em;
}

/*Submit Button*/

div#submit_btn_wrapper
{
	position:absolute;
	bottom: -7%;
	right: 4%;
	width: 20%;
	height: 7%;
}


div#submit_btn_wrapper img
{
	display: block;
	width: 100%;
	height: 100%;
}

div#submit_cont
{
	position:absolute;
	top: 2%;
	left: 2%;
	width: 30%;
	height: 90%;

}

.submit
{
	display: block;
	width: 100%;
	height: 100%;
	
}

#submit_img
{
	background-decorator: image;
	background-image: select.png;
	
}
button#submit_btn:hover #submit_img
{
	background-decorator: image;
	background-image: select-pos-h.png;
}

button#submit_btn:active #submit_img
{
	background-decorator: image;
	background-image: select-pos-o.png;
}

.button_text_special
{
	text-align: left;
}

.button_text_special p
{
	display: block;
	margin-left: 100%;
	margin-top: -80%;
	position: absolute;
}

/*Common Text*/

div#common_text_wrapper
{
	width: 50%;
	height: 25%;
	position: absolute;
	bottom: 0;
	left: 0;
}

div#common_text_wrapper img
{
	display: block;
	width: 100%;
	height: auto;
}

div#common_text
{
	width: 94%;
	height: 92%;
	position: absolute;
	top: 1%;
	left: 5%;
	overflow: scroll;
}

/*Bottom Panel Elements*/

div#bottom_panel_wrapper
{
	width: 25%;
	height: auto;
	overflow: hidden;
	position: absolute;
	bottom: 0;
	right: 0;
}

div#bottom_panel_a
{
	width: 34%;
	height: auto;
	position: relative;
	float: right;
}

div#bottom_panel_a img
{
	display: block;
	width: 100%;
	height: auto;
}

div#a_panel_wrapper
{
	position: absolute;
	bottom: 0;
}

div#launch_cont
