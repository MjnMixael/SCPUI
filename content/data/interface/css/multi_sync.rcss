	width: 33%;
	height: auto;
	position: relative;
	float: left;
}

div#bottom_panel_b img
{
	display: block;
	width: 100%;
	height: auto;
}

div#b_panel_wrapper
{
	position: absolute;
	bottom: 0;
}

div#accept_cont
{
	width: 80%;
	height: auto;

}

.accept
{
	display: block;
	width: 100%;
	height: auto;

}

#accept_img
{
	background-decorator: image;
	background-image: decline.png;
}
button#accept_btn:hover #accept_img
{
	background-decorator: image;
	background-image: decline-h.png;
}

button#accept_btn:active #accept_img
{
	background-decorator: image;
	background-image: decline-o.png;
}

div#bottom_panel_c
{
	width: 33%;
	height: auto;
	position: relative;
	float: left;
}

div#bottom_panel_c img
{
	display: block;
	width: 100%;
	height: auto;
}

div#c_panel_wrapper
{
	position: absolute;
	bottom: 0;
}

div#kick_cont
{
	width: 80%;
	height: auto;

}

.kick
{
	display: block;
	width: 100%;
	height: auto;

}

#kick_img
{
	background-decorator: image;
	background-image: cancel.png;
}
button#kick_btn:hover #kick_img
{
	background-decorator: image;
	background-image: cancel-h.png;
}

button#kick_btn:active #kick_img
{
