body
{
	z-index: bottom;
	z-index: -1;
}

.below p
{
	padding-top:8%;
}

/*Squadwar button*/
div#squadwar_wrapper
{
	position:absolute;
	top: 10%;
	left: 3%;
	width: 10%;
	height: 3%;
}

div#squadwar_lock
{
	position: absolute;
	top: 9%;
	left: 2%;
	width: 12%;
	height: 5%;
	z-index: 10;
}

div#squadwar_wrapper img
{
	display: block;
	width: 100%;
	height: 100%;
}

.button_text_right_sw p
{
	position: absolute;
	margin-top: -75%;
	margin-left: 120%;
	width: 400%;
}

div#squadwar_cont
{
	position:absolute;
	top: 2%;
	left: 20%;
	width: 20%;
	height: 90%;
}

.squadwar
{
	display: block;
	width: 100%;
	height: 100%;
	
}

#squadwar_img
{
	background-decorator: image;
	background-image: round.png;
	
}
button#squadwar_btn:hover #squadwar_img
{
	background-decorator: image;
	background-image: round-h.png;
}

button#squadwar_btn:checked #squadwar_img
{
	background-decorator: image;
	background-image: round-o.png;
}

button#squadwar_btn:active #squadwar_img
{
	background-decorator: image;
	background-image: round-o.png;
}

