/*Dropdown Selector*/

div#dropdown_template
{
	position: absolute;
	bottom: 20%;
	left: 5%;
	width: 30%;
}

div#dropdown_template img
{
	display: block;
	width: 100%;
	height: auto;
}

div#horz_panel_wrapper
{
	position: absolute;
	bottom: 6%;
	padding-right: 8%;
}

#horz_panel_wrapper .button_text_left p
{
	width: 100%;
	margin-left: -100%;
	margin-top: -85%;
}

.btn_panel_horz img
{
	display: block;
	width: 100%;
	height: auto;

}

.title_panel_horz
{
	width: 67%;
	height: auto;
	position: relative;
	border-bottom-width: 2px;
	border-bottom-color: grey;
}

#dropdown_cont
{
	width: 100%;
}

/*Locking Elements*/

#password_lock
{
	position: absolute;
	top: 43%;
	left: 4%;
	width: 60%;
	height: 8%;
	z-index: 99;
}

#rank_lock
{
	position: absolute;
	top: 65%;
	left: 4%;
	width: 40%;
	height: 17%;
	z-index: 99;
}

/*Bottom Panel Elements*/

div#bottom_panel_wrapper
{
	width: 15%;
	height: auto;
	overflow: hidden;
	position: absolute;
	bottom: 0;
	right: 0;
}

div#bottom_panel_a
{
	width: 66%;
	height: auto;
	position: relative;
	float: right;
}

div#bottom_panel_a img
{
	display: block;
	width: 100%;
	height: auto;
}

div#bottom_panel_b
{
	width: 34%;
	height: auto;
	margin-top: 1.1%;
	position: relative;
	float: left;
}

div#bottom_panel_b img
{
	display: block;
	width: 100%;
	height: auto;

}

