body
{
	z-index: bottom;
	z-index: -1;
}

.below p
{
	padding-top:8%;
}

#create_game_title
{
	margin: 4%;
}

#form_wrapper
{
	width: 80%;
	height: 60%;
	position: absolute;
	top: 8%;
	left: 5%;
}

div#form_wrapper img
{
	display: block;
	width: 100%;
	height: 100%;
}

#game_title_wrapper
{
	width: 80%;
	height: 30%;
}

#game_title_img
{
	width: 4%;
	height: 60%;
}

#game_title_header
{
	position: relative;
	top: 2%;
	left: 5%;
}

#game_title_input
{
	position: relative;
	top: -13%;
	left: 4%;
	width: 60%;
	height: 25%;
}

#title_input
{
	position: absolute;
	top: 2%;
	left: 2%;
	width: 95.5%;
	height: 88%;
	background-decorator: image;
	background-image: input-box.png;
	border-width: 1px;
	border-color: #676767;
	z-index: 10;
}

#game_type_wrapper
{
	width: 80%;
	height: 50%;
}

#game_type_img
{
	width: 4%;
	height: 80%;
}

#game_type_header
{
	position: relative;
	top: -2%;
	left: 0%;
}

#game_type_input
{
	position: relative;
	top: -30%;
	left: 4%;
	width: 60%;
	height: 15%;
}

#password_input
{
	position: absolute;
	top: 2%;
	left: 2%;
	width: 95.5%;
	height: 88%;
	background-decorator: image;
	background-image: input-box.png;
	border-width: 1px;
	border-color: #676767;
	z-index: 10;
}

/*Game Type Buttons*/

div#game_type_buttons
{
	position:relative;
	top: -45%;
	left: -0.5%;
	width: 10%;
	height: 6%;
}


div#game_type_buttons img
{
	display: block;
	width: 100%;
	height: 100%;
}

.button_text_right
{
	text-align: center;
}

.button_text_right p
{
	margin-top: -90%;
}

div#open_cont
{
	position:relative;
	width: 30%;
	height: 90%;
	margin-left: 5%;
}

.open
{
	display: block;
	width: 100%;
	height: 100%;

}

#open_img
{
	background-decorator: image;
	background-image: bullet-r.png;

}
button#open_btn:hover #open_img
{
	background-decorator: image;
	background-image: bullet-r-h.png;
}

button#open_btn:checked #open_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

button#open_btn:active #open_img
{
	background-decorator: image;
	background-image: bullet-r-o.png;
}

