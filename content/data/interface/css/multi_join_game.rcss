body
{
	z-index: bottom;
	z-index: -1;
}

.below p
{
	padding-top:8%;
}

.list_element
{
	display: block;
	color: #7b7b7b;
	width: 100%;
}
.list_element:hover
{
	color: #abbbab;
}
.list_element:checked
{
	color: #6f92f5;

}

.list_element:active
{
	color: #a5b6e6;

}

#games_list_wrapper
{
	width: 70%;
	height: 60%;
	position: absolute;
	top: 6%;
	right: 15%;
}

div#games_list_wrapper img
{
	display: block;
	width: 100%;
	height: 100%;
}

#games_list_header
{
	width: 94%;
	height: 5%;
	position: absolute;
	top: 5%;
	left: 2%;
}

#games_list
{
	width: 94%;
	height: 87%;
	position: absolute;
	top: 10%;
	left: 2%;
	overflow: scroll;
}

.game_item
{
	width: 15%;
	float: left;
}

.game_status
{
	width: 11%;
}

.game_type
{
	width: 8%;
}

.game_server
{
	width: 50%;
}

/*Refresh Buttons*/

div#refresh_container
{
	position:absolute;
	bottom: -5%;
	left: 4%;
	width: 15%;
	height: 8%;
}


div#refresh_container img
{
	display: block;
	width: 100%;
	height: 100%;
}

div#refresh_wrapper
{
	position: absolute;
	top: 0;
	left: 0;
}

.button_text_right
{
	text-align: center;
}

div#refresh_cont
{
	position:relative;
	float: left;
	width: 30%;
	height: 90%;
	margin-left: 5%;
}

.refresh
{
	display: block;
	width: 100%;
	height: 100%;
	
}

#refresh_img
{
	background-decorator: image;
	background-image: replay.png;
	
}
button#refresh_btn:hover #refresh_img
{
	background-decorator: image;
	background-image: replay-pos-h.png;
}

button#refresh_btn:active #refresh_img
{
	background-decorator: image;
	background-image: replay-pos-o.png;
}

