/*Games List Scrollbar*/

#games_list scrollbarvertical
{
	scrollchange-sound: 9;
	width: 4%;
	position: absolute;
	right: -2%;
	z-index: 5;
}

#games_list scrollbarvertical slidertrack
{
	background-decorator: image;
	background-image: scroll-bar-line.png;
	width: 8%;
	margin-left: 46%;
}

#games_list scrollbarvertical sliderbar
{
	width: 65%;
	height: 5%;
	background-decorator: image;
	background-image: scroll-button.png;
	margin-left: 20%;
}

#games_list scrollbarvertical sliderbar:hover
{
	background-image: scroll-button-h.png;
}

#games_list scrollbarvertical sliderbar:active
{
	background-image: scroll-button-o.png;
}

#games_list scrollbarvertical sliderarrowdec
{
	width: 100%;
	height: 9%;
	margin-top: -8%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-up.png;
}

#games_list scrollbarvertical sliderarrowinc
{
	width: 100%;
	height: 9%;
	margin-bottom: -1%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-down.png;
}

#games_list scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-dbl-up-h.png;
}

#games_list scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-dbl-down-h.png;
}

#games_list scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-dbl-up-o.png;
}

#games_list scrollbarvertical sliderarrowinc:active
{
	icon-image: arrow-dbl-down-o.png;
}

/*Common Text Scrollbar*/

#common_text scrollbarvertical
{
	scrollchange-sound: 9;
	width: 6%;
	position: absolute;
	left: -6%;
	z-index: 5;
}

#common_text scrollbarvertical slidertrack
{
	display: none;
}

#common_text scrollbarvertical sliderbar
{
	display: none;
}

#common_text scrollbarvertical sliderbar:hover
{
	display: none;
}

#common_text scrollbarvertical sliderbar:active
{
	display: none;
}

#common_text scrollbarvertical sliderarrowdec
{
	width: 100%;
	height: 23%;
	margin-top: 44%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-up.png;
}

#common_text scrollbarvertical sliderarrowinc
{
	width: 100%;
	height: 23%;
	margin-bottom: -4%;
	mouseover-sound: 17;
	click-sound: 0;
	icon-decorator: image;
	icon-image: arrow-dbl-down.png;
}

#common_text scrollbarvertical sliderarrowdec:hover
{
	icon-image: arrow-dbl-up-h.png;
}

#common_text scrollbarvertical sliderarrowinc:hover
{
	icon-image: arrow-dbl-down-h.png;
}

#common_text scrollbarvertical sliderarrowdec:active
{
	icon-image: arrow-dbl-up-o.png;
}

#common_text scrollbarvertical sliderarrowinc:active
